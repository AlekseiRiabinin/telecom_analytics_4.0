{
  "version": 8,
  "name": "Dubai",
  "sources": {
    "dubai": {
      "type": "vector",
      "tiles": ["http://localhost:8085/maps/dubai/{z}/{x}/{y}.pbf"],
      "minzoom": 0,
      "maxzoom": 20
    }
  },
  "layers": [
    {
      "id": "buildings",
      "type": "fill",
      "source": "dubai",
      "source-layer": "buildings",
      "paint": { "fill-color": "#888888", "fill-opacity": 0.7 }
    },
    {
      "id": "roads",
      "type": "line",
      "source": "dubai",
      "source-layer": "roads",
      "paint": { "line-color": "#ffffff", "line-width": 1 }
    }
  ]
}
